{
    "version": 2,
    "builds": [
      {
        "buildCommand": "concurrently \"cd server && npm run dev\" \"cd client && npm start\""
      },
      {
        "src": "client/package.json",
        "use": "@vercel/static-build"
      },
      {
        "src": "server/**/*.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/api/$1"
      }
    ]
  }
  